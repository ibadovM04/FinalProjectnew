@model List<SliderDto>

    <div>
    @foreach(var slider in Model)
    {
        <div class="site-blocks-cover" style="background-image:url(@slider.BackroundImageUrl);" data-aos="fade">
            <div class="container">
                <div class="row align-items-start align-items-md-center justify-content-end">
                    <div class="col-md-5 text-center text-md-left pt-5 pt-md-0">
                        <h1 class="mb-2">@slider.Title</h1>
                        <div class="intro-text text-center text-md-left">
                            <p class="mb-4">@slider.Text </p>
                            <p>
                                <a href="#" class="btn btn-sm btn-primary">@slider.Link</a>
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        
    }
    
</div>